{
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "globals": {
    "window": true
  },
  "rules": {
    "no-underscore-dangle": "off",
    "prettier/prettier": "error",
    "no-plusplus": "off",
    "no-console": "off",
    "prefer-const": "off",
    "no-param-reassign": ["error", { "props": false }]
  }
}
